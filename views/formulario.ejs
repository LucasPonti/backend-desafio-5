<h1 style="color:blue;">Ingrese datos</h1>
<br>

<form action="/productos" method="post">

    <div class="form-group">
        <label for="producto"><b>Producto</b></label>
        <input id="producto" class="form-control" type="text" name="producto">
    </div>

    <div class="form-group">
        <label for="precio"><b>Precio</b></label>
        <input id="precio" class="form-control" type="number" name="precio">
    </div>

    <div class="form-group">
        <label for="url"><b>URL</b></label>
        <input id="url" class="form-control" type="text" name="url">
    </div>

    <button class="btn btn-success mt-3 mb-5">Enviar</button>

</form>

<form action="/productos" method="get">
    <button class="btn btn-success mt-3 mb-5">Ir a Historial</button>
</form>